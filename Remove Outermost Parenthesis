class Solution:
    def removeOuterParentheses(self, S: str) -> str:
        cnt = 0
        res = ''
        for i in range(len(S)):
            if S[i] == '(':
                cnt += 1
                if cnt > 1:
                    res += '('
            else:
                cnt -= 1
                if cnt != 0:
                    res += ')'
        return res    
            
